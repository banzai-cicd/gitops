@Library('Banzai@gitops') _
banzai {
    sshCreds = ['server-ssh-creds-jenkins-service', 'git-ssh-creds-jenkins-service']
    gitOps = [
        autoDeploy: [
            /develop/ : 'dev'
        ],
        envs: [
            'dev' : [
                watchers: 'banzai-watchers',
            ],
            'qa' : [
                approvers: 'banzai-approvers',
                watchers: 'banzai-watchers'
            ]
        ]
    ]
}

